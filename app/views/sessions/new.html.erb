<h1>Log In</h1><br>
<hr><br>
<div class="login_form">
<%= form_for(:session, url: login_path) do |f| %>
<%= f.label :email %>
<%= f.text_field :email %>
<br><br>

<%= f.label :password %>
<%= f.password_field :password %>
<br>

<%= f.submit "Log In" %>
<% end %> 

<br>
<p>New user? <%= link_to "Sign up now!", new_user_path_url %></p>
</div>